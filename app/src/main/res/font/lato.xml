<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!--
        android: is for API 26+, app: is for API 16+
    -->
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/lato_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/lato_bold"/>

    <font
        android:fontStyle="normal"
        android:fontWeight="900"
        android:font="@font/lato_heavy"
        app:fontStyle="normal"
        app:fontWeight="900"
        app:font="@font/lato_heavy"/>

    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/lato_medium"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/lato_medium" />

    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/lato_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/lato_semibold"/>
</font-family>